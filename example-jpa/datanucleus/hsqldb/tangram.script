CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE CODE(ID BIGINT NOT NULL,ANNOTATION VARCHAR(255),CODE VARCHAR(32000),MIMETYPE VARCHAR(255),CONSTRAINT CODE_PK PRIMARY KEY(ID))
CREATE MEMORY TABLE ARTICLE(ID BIGINT NOT NULL,"TEXT" LONGVARBINARY,KEYWORDS VARCHAR(255),SHORTTITLE VARCHAR(255),TITLE VARCHAR(255),CONSTRAINT ARTICLE_PK PRIMARY KEY(ID))
CREATE MEMORY TABLE SEQUENCE_TABLE(SEQUENCE_NAME VARCHAR(255) NOT NULL,NEXT_VAL BIGINT NOT NULL,CONSTRAINT SEQUENCE_TABLE_PK PRIMARY KEY(SEQUENCE_NAME))
CREATE MEMORY TABLE CONTAINER(ID BIGINT NOT NULL,KEYWORDS VARCHAR(255),SHORTTITLE VARCHAR(255),TITLE VARCHAR(255),CONSTRAINT CONTAINER_PK PRIMARY KEY(ID))
CREATE MEMORY TABLE IMAGEDATA(ID BIGINT NOT NULL,"DATA" LONGVARBINARY,HEIGHT VARCHAR(255),MIMETYPE VARCHAR(255),WIDTH VARCHAR(255),KEYWORDS VARCHAR(255),SHORTTITLE VARCHAR(255),TITLE VARCHAR(255),CONSTRAINT IMAGEDATA_PK PRIMARY KEY(ID))
CREATE MEMORY TABLE TOPIC(ID BIGINT NOT NULL,TEASER LONGVARBINARY,THUMBNAIL_ID BIGINT,KEYWORDS VARCHAR(255),SHORTTITLE VARCHAR(255),TITLE VARCHAR(255),CONSTRAINT TOPIC_PK PRIMARY KEY(ID),CONSTRAINT TOPIC_FK1 FOREIGN KEY(THUMBNAIL_ID) REFERENCES IMAGEDATA(ID))
CREATE INDEX TOPIC_N49 ON TOPIC(THUMBNAIL_ID)
CREATE MEMORY TABLE CONTAINER_TOPIC(CONTAINER_ID BIGINT NOT NULL,CONTENTS_ID BIGINT,CONSTRAINT CONTAINER_TOPIC_FK1 FOREIGN KEY(CONTAINER_ID) REFERENCES CONTAINER(ID),CONSTRAINT CONTAINER_TOPIC_FK2 FOREIGN KEY(CONTENTS_ID) REFERENCES TOPIC(ID))
CREATE INDEX CONTAINER_TOPIC_N50 ON CONTAINER_TOPIC(CONTAINER_ID)
CREATE INDEX CONTAINER_TOPIC_N49 ON CONTAINER_TOPIC(CONTENTS_ID)
CREATE MEMORY TABLE TOPIC_ARTICLE(TOPIC_ID BIGINT NOT NULL,ELEMENTS_ID BIGINT,CONSTRAINT TOPIC_ARTICLE_FK1 FOREIGN KEY(TOPIC_ID) REFERENCES TOPIC(ID),CONSTRAINT TOPIC_ARTICLE_FK2 FOREIGN KEY(ELEMENTS_ID) REFERENCES ARTICLE(ID))
CREATE INDEX TOPIC_ARTICLE_N50 ON TOPIC_ARTICLE(TOPIC_ID)
CREATE INDEX TOPIC_ARTICLE_N49 ON TOPIC_ARTICLE(ELEMENTS_ID)
CREATE MEMORY TABLE TOPIC_TOPIC(TOPIC_ID BIGINT NOT NULL,SUBTOPICS_ID BIGINT,CONSTRAINT TOPIC_TOPIC_FK1 FOREIGN KEY(TOPIC_ID) REFERENCES TOPIC(ID),CONSTRAINT TOPIC_TOPIC_FK2 FOREIGN KEY(SUBTOPICS_ID) REFERENCES TOPIC(ID))
CREATE INDEX TOPIC_TOPIC_N49 ON TOPIC_TOPIC(TOPIC_ID)
CREATE INDEX TOPIC_TOPIC_N50 ON TOPIC_TOPIC(SUBTOPICS_ID)
CREATE MEMORY TABLE TOPIC_CONTAINER(TOPIC_ID BIGINT NOT NULL,RELATEDCONTAINERS_ID BIGINT,CONSTRAINT TOPIC_CONTAINER_FK1 FOREIGN KEY(TOPIC_ID) REFERENCES TOPIC(ID),CONSTRAINT TOPIC_CONTAINER_FK2 FOREIGN KEY(RELATEDCONTAINERS_ID) REFERENCES CONTAINER(ID))
CREATE INDEX TOPIC_CONTAINER_N50 ON TOPIC_CONTAINER(TOPIC_ID)
CREATE INDEX TOPIC_CONTAINER_N49 ON TOPIC_CONTAINER(RELATEDCONTAINERS_ID)
CREATE MEMORY TABLE ROOTTOPIC(ID BIGINT NOT NULL,LOGO_ID BIGINT,TEASER LONGVARBINARY,THUMBNAIL_ID BIGINT,KEYWORDS VARCHAR(255),SHORTTITLE VARCHAR(255),TITLE VARCHAR(255),CONSTRAINT ROOTTOPIC_PK PRIMARY KEY(ID),CONSTRAINT ROOTTOPIC_FK2 FOREIGN KEY(LOGO_ID) REFERENCES IMAGEDATA(ID),CONSTRAINT ROOTTOPIC_FK1 FOREIGN KEY(THUMBNAIL_ID) REFERENCES IMAGEDATA(ID))
CREATE INDEX ROOTTOPIC_N50 ON ROOTTOPIC(LOGO_ID)
CREATE INDEX ROOTTOPIC_N49 ON ROOTTOPIC(THUMBNAIL_ID)
CREATE MEMORY TABLE ROOTTOPICBOTTOMLINKS(ROOTTOPIC_ID BIGINT NOT NULL,BOTTOMLINKS_ID BIGINT,CONSTRAINT ROOTTOPICBOTTOMLINKS_FK2 FOREIGN KEY(BOTTOMLINKS_ID) REFERENCES TOPIC(ID),CONSTRAINT ROOTTOPICBOTTOMLINKS_FK1 FOREIGN KEY(ROOTTOPIC_ID) REFERENCES ROOTTOPIC(ID))
CREATE INDEX ROOTTOPICBOTTOMLINKS_N49 ON ROOTTOPICBOTTOMLINKS(BOTTOMLINKS_ID)
CREATE INDEX ROOTTOPICBOTTOMLINKS_N50 ON ROOTTOPICBOTTOMLINKS(ROOTTOPIC_ID)
CREATE MEMORY TABLE ROOTTOPICCSS(ROOTTOPIC_ID BIGINT NOT NULL,CSS_ID BIGINT,CONSTRAINT ROOTTOPICCSS_FK2 FOREIGN KEY(CSS_ID) REFERENCES CODE(ID),CONSTRAINT ROOTTOPICCSS_FK1 FOREIGN KEY(ROOTTOPIC_ID) REFERENCES ROOTTOPIC(ID))
CREATE INDEX ROOTTOPICCSS_N50 ON ROOTTOPICCSS(CSS_ID)
CREATE INDEX ROOTTOPICCSS_N49 ON ROOTTOPICCSS(ROOTTOPIC_ID)
CREATE MEMORY TABLE ROOTTOPICJS(ROOTTOPIC_ID BIGINT NOT NULL,JS_ID BIGINT,CONSTRAINT ROOTTOPICJS_FK1 FOREIGN KEY(ROOTTOPIC_ID) REFERENCES ROOTTOPIC(ID),CONSTRAINT ROOTTOPICJS_FK2 FOREIGN KEY(JS_ID) REFERENCES CODE(ID))
CREATE INDEX ROOTTOPICJS_N50 ON ROOTTOPICJS(JS_ID)
CREATE INDEX ROOTTOPICJS_N49 ON ROOTTOPICJS(ROOTTOPIC_ID)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
INSERT INTO CODE VALUES(1,'screen','body {\u000a  background-color: #506070;\u000a  color: #607080;\u000a  font-family: Arial, Helvetica;\u000a  padding-top: 5px;\u000a  padding-bottom: 5px;\u000a  padding-left: 10px;\u000a  padding-right: 10px;\u000a  vertical-align: bottom;\u000a}\u000a\u000adiv {\u000a  margin: 0;\u000a  padding: 0;\u000a}\u000a\u000ap {\u000a  margin: 0;\u000a  padding: 0;\u000a}\u000a\u000aa {\u000a  text-decoration: none;\u000a  color: #FFFFFF;\u000a}\u000a\u000aa:link { }\u000a\u000aa:visited { }\u000a\u000aa:hover { color: #909090; }\u000a\u000aa:focus { color: #0000FF; }\u000a\u000aa:active { color: #FF0000; }\u000a\u000a.header a {\u000a  color: #D0B080;\u000a}\u000a\u000a\u000ah1 { font-size: 14pt; }\u000a\u000a.header {\u000a  background-color: #FFFFFF;\u000a  color: #D0B080;\u000a  height: 60px;\u000a  font-size: 20pt;\u000a  font-weight: bold;\u000a  text-align: right;\u000a  vertical-align: bottom;\u000a  padding: 10px;\u000a}\u000a\u000a.infobox {\u000a  background-color: #D0B080;\u000a  height: 24px;\u000a  padding-top: 5px;\u000a  padding-left: 5px;\u000a  margin: 0;\u000a}\u000a\u000a.box {\u000a  padding: 5px;\u000a  width: 300px;\u000a  float: left;\u000a  color: #909090;\u000a}\u000a\u000a.text {\u000a  color: #FFFFE0;\u000a}\u000a\u000a.clear {\u000a  clear: both;\u000a}\u000a','text/css')
